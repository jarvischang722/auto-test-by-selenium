{
  "output": "./output",
  "helpers": {
    "WebDriverIO": {
      "url": "http://localhost",
      "browser": "chrome",
      "smartWait": 1000,
      "windowSize": "maximize",
      "uniqueScreenshotNames": true
    }
  },
  "mocha": {
    "reporterOptions": {
      "reportDir": "reports",
      "uniqueScreenshotNames": "true"
    }
  },
  "bootstrap": false,
  "teardown": null,
  "hooks": [],
  "tests": "./test/specs/*_test.js",
  "timeout": 10000,
  "name": "auto-test-webdriver"

}